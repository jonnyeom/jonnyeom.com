{
    "name": "jonnyeom/jonnyeom.com",
    "license": "proprietary",
    "type": "project",
    "description": "Jonnyeom Personal Site",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "require": {
        "php": "^8.2",
        "api-platform/core": "^2.7.16",
        "doctrine/annotations": "^2.0.1",
        "league/commonmark": "^2",
        "leogout/seo-bundle": "^1.3",
        "nelmio/cors-bundle": "^2.3.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.23.1",
        "spatie/commonmark-highlighter": "^3.0",
        "spatie/yaml-front-matter": "^2.0.7",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2.2.5",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/monolog-bundle": "^3.8.0",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webpack-encore-bundle": "^1.17.2",
        "symfony/yaml": "6.3.*"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0.0",
        "phpstan/phpstan": "^1.10.39",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-doctrine": "^1.3.43",
        "phpstan/phpstan-symfony": "^1.3.4",
        "phpunit/phpunit": "^9.6.13",
        "rector/rector": "^0.18.6",
        "squizlabs/php_codesniffer": "^3.7.2",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "^1.51.1",
        "symfony/phpunit-bridge": "6.3.*",
        "symfony/stopwatch": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*",
            "docker": true
        }
    }
}
